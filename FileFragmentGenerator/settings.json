{
    "fileType": ["hwp", "jpg", "mp3"],

    "inputDirectory": {
        "hwp": "./input/hwp_train",
        "jpg": "./input/jpg_train",
        "mp3": "./input/mp3_train"
    },

    "typeKey": {
        "hwp": 0,
        "jpg": 1,
        "mp3": 2
    },

    "numOfGroups": 3,

    "outputDirectory": "./train_data",
    "outputFileName": "hwp_jpg_mp3_frequency_train_",
    "shuffleOutput": true,

    "settings": {
        "gram": 1,
        "fragmentSize": 4096,
        "totalFragmentsPerType": 35000,
        "fragmentsPerCSV": 100
    }
}
